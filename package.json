{
  "name": "@grossb/reference-book",
  "version": "0.2.0",
  "dependencies": {
    "@grossb/react-data-table": "^0.4.3",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build-project": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "copy:styles": "copyfiles -f src/ReferenceBook/styles/** dist/styles && copyfiles -f src/ReferenceBook/components/styles/** dist/components/styles",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.prod.json",
    "postbuild": "npm run copy:styles",
    "pub": "npm run build && npm publish --access=public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "copyfiles": "^2.4.1",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}